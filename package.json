{
  "name": "angular-14-admin-boiler",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compilar-dev": "node dist/vaciarCarpetaOrigen.mjs && ng build marcaBlanca --configuration=desarrollo && node dist/copiarCarpetaDestino.mjs dev && node dist/vaciarCarpetaOrigen.mjs",
    "compilar-dev:stats": "node dist/vaciarCarpetaOrigen.mjs && ng build marcaBlanca --configuration=desarrollo --stats-json && node dist/copiarCarpetaDestino.mjs dev && node dist/vaciarCarpetaOrigen.mjs",
    "compilar-homo": "node dist/vaciarCarpetaOrigen.mjs && ng build marcaBlanca --configuration=homologacion && node dist/copiarCarpetaDestino.mjs homo && node dist/vaciarCarpetaOrigen.mjs",
    "compilar-prod": "node dist/vaciarCarpetaOrigen.mjs && ng build marcaBlanca --configuration=production && node dist/copiarCarpetaDestino.mjs prod && node dist/vaciarCarpetaOrigen.mjs",
    "compilar-all": "npm run compilar-dev && npm run compilar-homo && npm run compilar-prod",
    "analizar-dev": "webpack-bundle-analyzer dist/dev/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0-next.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.0.0-next.0",
    "@angular/compiler": "^14.0.0-next.0",
    "@angular/core": "^14.0.0-next.0",
    "@angular/forms": "^14.0.0-next.0",
    "@angular/material": "^14.2.0",
    "@angular/material-moment-adapter": "^14.2.3",
    "@angular/platform-browser": "^14.0.0-next.0",
    "@angular/platform-browser-dynamic": "^14.0.0-next.0",
    "@angular/router": "^14.0.0-next.0",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@geoman-io/leaflet-geoman-free": "^2.13.0",
    "@types/leaflet": "^1.7.11",
    "@types/leaflet-editable": "^1.2.1",
    "@types/leaflet.awesome-markers": "^2.0.25",
    "bootstrap": "^5.2.0",
    "chart.js": "^4.1.1",
    "chartjs-plugin-annotation": "^2.1.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-zoom": "^2.0.0",
    "file-saver": "^2.0.5",
    "glyphicons": "^0.2.0",
    "ionicons": "^4.6.3",
    "leaflet": "^1.8.0",
    "leaflet-ant-path": "^1.3.0",
    "leaflet-drift-marker": "^2.0.0",
    "leaflet-editable": "^1.2.0",
    "leaflet.awesome-markers": "^2.0.5",
    "material-icons": "^1.11.10",
    "moment": "^2.29.4",
    "ng2-currency-mask": "^13.0.3",
    "ngx-mask": "^14.2.4",
    "ngx-material-timepicker": "^5.5.3",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.0",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.0.0-next.0",
    "@types/file-saver": "^2.0.5",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
